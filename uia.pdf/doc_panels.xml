<?xml version="1.0" encoding="UTF-8"?>
<layout>
	<grid name="image">
		<columns>
			<column width="100%" bind="imagePath" background="232,232,232" cellRenderer="uia.pdf.grid.ImageCellRenderer"><text></text></column>
		</columns>
	</grid>
	<grid name="panel">
		<columns>
			<column width="20%" bind="col1" background="232,232,232"><text></text></column>
			<column width="30%" bind="value1" cellRenderer="uia.pdf.grid.ImageCellRenderer"><text></text></column>
			<column width="20%" bind="col2" background="232,232,232"><text></text></column>
			<column width="+100%" bind="value2"><text></text></column>
		</columns>
	</grid>
	<grid name="header" fontSize="9">
		<columns>
			<column width="20%" bind="col1"><text></text></column>
			<column width="30%" bind="value1"><text></text></column>
			<column width="20%" bind="col2"><text></text></column>
			<column width="+100%" bind="value2"><text></text></column>
		</columns>
	</grid>
	<grid name="footer" fontSize="9">
		<columns>
			<column width="20%" bind="col1"><text></text></column>
			<column width="30%" bind="value1"><text></text></column>
			<column width="20%" bind="col2"><text></text></column>
			<column width="+100%" bind="value2"><text></text></column>
		</columns>
	</grid>
</layout>
